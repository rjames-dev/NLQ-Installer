{
  "name": "nlq-platform-dashboard",
  "version": "1.0.0",
  "description": "Unified platform dashboard for NLQ System installation, management, and operation",
  "type": "module",
  "main": "server.js",
  "scripts": {
    "dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "server:dev": "node --watch server.js",
    "client:dev": "vite",
    "client:build": "vite build",
    "client:preview": "vite preview",
    "build": "npm run client:build",
    "start": "node server.js",
    "preview": "vite preview"
  },
  "keywords": [
    "nlq",
    "installation",
    "dashboard",
    "docker",
    "platform"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.0",
    "dockerode": "^4.0.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-http-proxy": "^2.0.0",
    "express-session": "^1.17.3",
    "js-yaml": "^4.1.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.0.0",
    "concurrently": "^8.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "vite": "^4.4.0",
    "tailwindcss": "^3.3.0",
    "postcss": "^8.4.29",
    "autoprefixer": "^10.4.14",
    "lucide-react": "^0.263.1"
  }
}
